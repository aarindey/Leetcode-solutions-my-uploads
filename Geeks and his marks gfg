#include <iostream>
using namespace std;

int main() {
	//code
	int T;
	cin>>T;
	int N,X;
	while(T--)
	{
	    cin>>N;
	    cin>>X;
	    int arr[N];
	    int count=0;
	    for(int i=0;i<N;i++)
	    {
	    cin>>arr[i];
	    if(arr[i]>X)
	    count++;
	    }
	    cout<<count<<endl;
        
	}
	return 0;
}
